   <string notr="true">/* 整体背景与字体设置 */
QWidget {
    /* 背景色，可以根据需要调整 */
    background-color: #F5F5F5;
    
    /* 字体设置，可换成你需要的字体和大小 */
    font-family: &quot;微软雅黑&quot;;
    font-size: 12px;
}

/* 针对所有带 custom-btn 属性的 QPushButton */
QPushButton[class=&quot;custom-btn&quot;] {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #ffffff, stop:1 #f0f0f0);
    border: 1px solid #ddd;
    border-radius: 4px;
    padding: 5px;
    min-width: 80px;
    /* 移除不支持的 transition 属性 */
}

/* 悬停状态 */
QPushButton[class=&quot;custom-btn&quot;]:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #f8f8f8, stop:1 #e0e0e0);
    border-color: #ccc;
}

/* 按下状态 */
QPushButton[class=&quot;custom-btn&quot;]:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #e0e0e0, stop:1 #d0d0d0);
}

/* 按钮基础样式 */
QPushButton {
    background-color: #3498db; /* 主色调 */
    color: #ffffff;           /* 文字颜色 */
    border-radius: 5px;       /* 圆角 */
    padding: 5px 10px;        /* 内边距 */
    border: none;             /* 无边框 */
}

/* 按钮鼠标悬停与点击效果 */
QPushButton:hover {
    background-color: #2980b9;
}
QPushButton:pressed {
    background-color: #1f6391;
}

/* 文本输入框 */
QLineEdit {
    border: 1px solid #cccccc;
    border-radius: 4px;
    padding: 2px 4px;
    background-color: #ffffff;
    color: #333333;
}

/* 文本输入框获得焦点时边框高亮 */
QLineEdit:focus {
    border: 2px solid #3498db;
}

/* GroupBox 外观 */
QGroupBox {
    border: 1px solid gray; 
    border-radius: 5px;     
    margin-top: 1em;        
    background-color: #ffffff;
}

/* GroupBox 标题的位置和样式 */
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 5px;
    color: #333333;
    font-weight: bold; /* 标题加粗 */
}

/* Label 基本样式 */
QLabel {
    color: #333333;
    font-size: 12px;
}</string>
